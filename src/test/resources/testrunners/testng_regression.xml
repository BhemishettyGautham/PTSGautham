<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="PannaTimeSheets App Test Regression Suite"  verbose="4">
	
	
	<listeners>
		<listener
			class-name="com.qa.pts.listners.ExtentReportListners" />
		<listener
			class-name="com.qa.pts.listners.AllureReportListners" />
		<listener
			class-name="com.qa.pts.listners.AnnotationTransformer" />
			
	</listeners>

	<test name="Login Page Test">
		<classes>
			<class name="com.qa.pts.tests.LoginPageTest" />
		</classes>
	</test>
	
	<test name="Home Page Test">
		<classes>
			<class name="com.qa.pts.tests.HomePageTest" />
		</classes>
	</test>
	
	<test name="Time Entry Page Test">
		<classes>
			<class name="com.qa.pts.tests.TimeEntryPageTest" />
		</classes>
	</test>
	
	<test name="Users Page Test">
		<classes>
			<class name="com.qa.pts.tests.UsersPageTest" />
		</classes>
	</test>

	

</suite>